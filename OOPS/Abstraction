abstract class Vehicle{
    abstract void tyres();
    void use(){
        System.out.println("Used as the means of transport");
    }
}
class Car extends Vehicle{
    void tyres(){
        System.out.println("It has 4 tyres, with 1 spare tyre");
    }
}
public class Abs {
    public static void main(String[] args) {
        Car c1= new Car();
        c1.use();
        c1.tyres();
    }
}
