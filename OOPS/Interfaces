interface college{
    void attendanceCritera();
    void fees();
}
class PrivateCollege implements  college{
    public void attendanceCritera(){
    System.out.println("Has a  attendance criteria of 75 percent");
    }
public void fees(){
    System.out.println("Has high fees");
}
}
public class Intrface {
    public static void main(String args[]){
        PrivateCollege p1= new PrivateCollege();
        p1.fees();
        p1.attendanceCritera();
    }
}
