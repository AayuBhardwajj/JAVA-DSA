public class Tc1 {
    public static void main(String[] args) {
        int i=18;
        int j=0;
        try{
            j=i/j;
        } catch(Exception e){
            System.out.println("There might be an error");
        }
        System.out.println("Vo chhodo, Aur btao kya haal hai");
    }
}
