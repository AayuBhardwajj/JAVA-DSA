public class Tc2 {
    public static void main(String[] args) {
        int arr[]= new int[5];
        int i=9;
        int j=0;
        try{
            System.out.println(arr[9]);
            j=i/j;
        } catch(ArrayIndexOutOfBoundsException e){
            System.out.println("Bhai itne toh array me elements bhi nahi hai");
        }
        catch(ArithmeticException e){
            System.out.println("Behnchod 0 se konsa number divide hota");
        }
    }
}
