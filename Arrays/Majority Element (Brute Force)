public class X{
public static void main(String[] args){
int arr[]={2,2,3,3,1,2,3,3,3};
findBrueteApproach(arr);
}
public static void findBrueteApproach(int arr[]){
for(int i=0;i<arr.length;i++){
int x=0;
for(int j=0;j<arr.length;j++){
if(arr[j]==arr[i]){
x++;
}
}
if(x>arr.length/2){
System.out.println("The majority element is: "+arr[i]);
return;
}
}
System.out.println("No majority element exists");
}
}
