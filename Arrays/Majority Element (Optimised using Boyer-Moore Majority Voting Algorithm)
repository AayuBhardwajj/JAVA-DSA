public class X{
public static void main(String[] args){
int arr[]={2,2,3,3,1,2,3,3,3};
optimizedApproach(arr);
}
public static void optimizedApproach(int arr[]){
  int el=0;
  int count=0;
  for(int i:arr){
  if(count==0){
  el=i;
  count=1;
}
else if(el==i){
count++;
} else{
count--;
}
}
System.out.println("Majority ELement is: "+el);
}

